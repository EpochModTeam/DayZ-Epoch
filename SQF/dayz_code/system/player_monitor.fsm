                {
                        name = "Phase_Two";
                        itemno = 19;
                        init = /*%FSM<STATEINIT""">*/"if (_debug == 1) then {diag_log [diag_tickTime,'Phase_Two'];};dayz_loadScreenMsg = localize 'str_login_characterData'; " \n
                         "" \n
                         "_worldspace = 	PVCDZ_plr_Login2 select 0;" \n
